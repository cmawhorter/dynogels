  "devDependencies": {
    "bunyan": "^1.8.5",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "coveralls": "^2.11.11",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.11.1",
    "istanbul": "^0.4.4",
